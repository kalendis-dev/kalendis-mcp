  "repository": {
    "type": "git",
    "url": "https://github.com/kalendis-dev/kalendis-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/kalendis-dev/kalendis-mcp/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@kalendis/utils": "^1.0.0",
    "@modelcontextprotocol/sdk": "^1.18.2"
  },
  "devDependencies": {
    "@types/node": "^20.19.19",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
